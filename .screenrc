defencoding UTF-8

caption string '%?%F%{=b}%:%{=}%?%{dm}::[%{w} %-w%{Y}%n %t%{-}%+w %{-}]::%{= dw} %h'
defflow off
defutf8 on
escape ^\^\
nethack off
shelltitle '% |%'
startup_message off
vbell off

bind 0 select 10
bind c screen 1
bind o focus
bind s split
bind ^x windowlist -b

setenv BCS_SCREEN 1
source .config/screen/local
screen 3
screen 2
screen 1
